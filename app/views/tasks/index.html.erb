<div class = "container m-3 p-2">
<h1>My tasks</h1>
<br>

<div class="form-check m-3">
  <% @tasks.each do |task|%>
  <input class="form-check-input" type="checkbox" value="" checked>
  <li style="list-style: none;">
    <%= link_to task.title, task_path(task), class: "listed_tasks"%>
  </li>

  <%end%>
</div>
  <%= link_to 'Create a new task', '/tasks/new', class: "btn btn-primary"%>
</div>